rule obj
  command=emcc -c $in -o $out -I. -I..
rule test
  command=emcc $in -o $out
rule lib
  command=emcc -s FULL_ES2=1 $in -o $out

build glmatrix/glmatrix.o: obj glmatrix/glmatrix.c
build glmatrix/test.o: obj glmatrix/test.c
build glmatrix/test.js: test core/core.o glmatrix/glmatrix.o glmatrix/test.o
